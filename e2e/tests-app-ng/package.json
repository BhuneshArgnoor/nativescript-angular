  "scripts": {
    "tslint": "tslint --config tslint.json 'app/**/*.ts'",
    "clean": "npx rimraf hooks node_modules platforms package-lock.json",
    "setup": "cd ../../nativescript-angular && npm run prep.apps && cd ../e2e/tests-app-ng && npm run clean",
