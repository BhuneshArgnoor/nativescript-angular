    "ns-verify-bundle": "ns-verify-bundle",
    "update-ns-webpack": "update-ns-webpack",
    "ios": "tns debug ios --env.aot --emulator --no-hmr",
    "android": "tns debug android --env.aot --emulator --no-hmr"
